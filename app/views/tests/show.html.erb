
<p>
  <b>title: </b> 
  <%= @test.title %>
</p>
<p>
  <b>level: </b> 
  <%= test_level(@test) %>
</p>
<% @test.questions.each do |question| %> 
  <div>
    <p><a href="<%= question_path(question.id) %>"><%= question.body %></a></p>
  </div>
<% end %>
<hr/>
<p>
<a href="<%= new_test_question_path(@test.id) %>">Create new question</a>
</p>


<p>
  <%= link_to 'Back', tests_path %> | 
  <%= link_to 'Edit', edit_test_path(@test) %>
</p>